{
  "name": "my-blog",
  "version": "1.0.0",
  "description": "React Mobx blog",
  "main": "src/app.js",
  "scripts": {
    "build:client": "webpack --config build/webpack.config.client.js",
    "build:server": "webpack --config build/webpack.config.server.js",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --config build/webpack.config.client.js --open",
    "dev:server": "nodemon server/server.js",
    "clear": "rimraf dist",
    "build": "npm run clear && npm run build:client && npm run build:server",
    "eslint": "eslint --ext .js --ext .jsx client/",
    "precommit": "npm run eslint"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andyxmq/my-blog.git"
  },
  "keywords": [
    "React",
    "mobx",
    "blog"
  ],
  "author": "andyxmq",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/andyxmq/my-blog/issues"
  },
  "homepage": "https://github.com/andyxmq/my-blog#readme",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.6",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-loose": "^8.0.0",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^5.2.0",
    "eslint": "^5.3.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "memory-fs": "^0.4.1",
    "nodemon": "^1.18.3",
    "react-hot-loader": "^4.3.4",
    "rimraf": "^2.6.2",
    "webpack": "^3.10.0",
    "webpack-dev-server": "^2.9.7",
    "webpack-merge": "^4.1.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "express": "^4.16.3",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "serve-favicon": "^2.5.0",
    "styled-components": "^3.4.1"
  }
}
